/* pages/mine/personal/personal.wxss */
page{
    background-color: #f8f8fa;
}
.container{
    display: flex;
    flex-direction: column;
}
.items{
    width: 100%;
    background-color: #fff;
    margin-top:20rpx;
}
.item{
    display: flex;
    flex-direction: row;
    width:90%;
    margin:0 5%;
    height:90rpx;
    background-color: #fff;
    border-bottom:1px solid #ececec;
}
.item>view{
    width: 30%;
    text-align: left;
    line-height: 90rpx;
    font-size: 30rpx;
}
.Didnot{
    width: 70%;
    line-height: 90rpx;
    color:#ccc;
}
.item .NeedSelected{
    width: 70%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.NeedSelected image{
    width:30rpx;
    height: 30rpx;
    margin-top: 30rpx;
}






