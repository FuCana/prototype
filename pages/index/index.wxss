/**index.wxss**/
@import "DongTai/Dongtai.wxss";
.head{
  position: relative;
  padding: 0;
}
.head image{
  width: 100%;
  height:300rpx;
}
.head .publish{
  position: absolute;
  width: 70rpx;
  height: 70rpx;
  right:20rpx;
  bottom: 20rpx;
  border-radius: 50%;
  background-color: #fff;
}
.publish image{
  width:50rpx;
  height: 50rpx;
  margin: 10rpx;
}
.on {
  color: #000;
  border-bottom: 5rpx solid #000;
}
.nav{
  display: flex;
  flex-direction: row;
  height: 100rpx;
  width: 100%;
}
.nav .tabbar{
  display: flex;
  flex-direction: column;
  width:33.33%; 
  text-align: center;
  padding:16rpx;
  height: 68rpx;
  font-size: 28rpx;
}
.nav image{
  width:50rpx;
  height:50rpx;
  margin: 0 auto;
}




